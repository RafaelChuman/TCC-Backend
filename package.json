{
  "name": "tcc-backend",
  "private": true,
  "version": "1.0.0",
  "description": "TCC-Backend",
  "main": "src/index.ts",
  "author": "Rafael Chuman Santana",
  "license": "MIT",
  "scripts": {
    "start": "node -r tsconfig-paths/register -r ts-node/register server.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn server.ts",
    "migration": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn ./node_modules/typeorm/cli.js migration:run --dataSource src/data-source.ts",
    "test": "jest",
    "test:watch": "jest --detectOpenHandles --watchAll",
    "test:coverage": "jest --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --coverage",
    "pretest": "cross-env NODE_ENV=test"
  },
  "dependencies": {
    "apexcharts": "^3.40.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "tsconfig": "^7.0.0",
    "tsconfig-paths": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.8.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typeorm": "^0.3.13",
    "typescript": "^4.9.5"
  }
}
